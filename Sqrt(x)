#include <math.h>
int mySqrt(int x) {
    int ans;
    double sqrt=exp(0.5*(log(x)));
    ans = (int)sqrt;
    long long a = (ans+1) * (ans+1);

    if (ans*ans > x)
    {
        return ans-1;
    }
    else if(a <= x)
    {
        return ans+1;
    }ALR 
    else{
        return ans;
    }
    
}
