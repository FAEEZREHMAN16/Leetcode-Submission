// Reverse Linked List
struct ListNode { int val; struct ListNode *next; };

struct ListNode* reverseList(struct ListNode* head){
    struct ListNode *prev = NULL, *cur = head;
    while(cur){
        struct ListNode *nx = cur->next;
        cur->next = prev;
        prev = cur;
        cur = nx;
    }
    return prev;
}
