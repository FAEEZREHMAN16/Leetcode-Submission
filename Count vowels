#include <stdio.h>
#include <ctype.h>

int main() {
    char s[100];
    int count = 0;

    printf("Enter string: ");
    fgets(s, sizeof(s), stdin);

    for(int i = 0; s[i] != '\0'; i++) {
        char ch = tolower(s[i]);
        if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
            count++;
    }

    printf("Vowel count = %d", count);
    return 0;
}
